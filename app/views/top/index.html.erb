  <% provide(:title, "Log in") %>

  <h1>新規登録</h1>
      <%= form_for(:session, url: signup_path) do |f| %>

        <%= f.label :email %>
        <%= f.email_field :email %>

        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.label :password再入力 %>
        <%= f.password_field :password_confirmation %>
        
        <%= f.submit "Log in" %>
      <% end %>


  <h1>Log in</h1>
      <%= form_for(:session, url: login_path) do |f| %>

        <%= f.label :email %>
        <%= f.email_field :email %>

        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.submit "Log in" %>
      <% end %>
