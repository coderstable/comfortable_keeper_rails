  <% provide(:title, "Log in") %>
  <h1>Log in</h1>
      <%= form_for(:session, url: login_path) do |f| %>

        <%= f.label :email %>
        <%= f.email_field :email %>

        <%= f.label :password %>
        <%= f.password_field :hashed_password %>

        <%= f.submit "Log in" %>
      <% end %>
